  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:worker\"",
    "start:backend": "node dist/index.js",
    "start:worker": "node dist/worker/index.js",
    "dev:worker": "nodemon  --ext ts --exec ts-node src/worker/index.ts",
    "dev:backend": "nodemon --ext ts --exec ts-node src/index.ts",
    "db:check": "drizzle-kit check",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "ts-node src/database/migrate.ts",
    "lint": "echo \"Error: no lint specified\" && exit 1",
    "check-types": "tsc --noEmit"
  },
  "keywords": [
    "recall",
    "backend"
  ],
